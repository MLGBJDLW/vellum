{
  "name": "@vellum/provider",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@vellum/shared": "workspace:*",
    "ai": "^4.1.0",
    "@ai-sdk/anthropic": "^1.1.0",
    "@ai-sdk/openai": "^1.1.0",
    "@ai-sdk/google": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
