      "toolCallId": "toolu_01ABC123",
      "arguments": "{\"path\": \"src/utils/helper.ts\", \"content\": \"export function greet(name: string): string {\\n  return `Hello, ${name}!`;\\n}\"}"
    },
    {
      "type": "tool_call_end",
      "toolCallId": "toolu_01ABC123"
    },
    {
      "type": "text",
      "content": "\n\nI've created a simple greeting function in `src/utils/helper.ts`."
    },
    {
      "type": "usage",
      "usage": {
        "inputTokens": 25,
        "outputTokens": 120
      }
    },
    {
      "type": "end",
      "stopReason": "end_turn"
    }
  ]
